   ],
   "source": [
    "x = sym.symbols('x')\n",
    "y = x**2 -3*x\n",
    "\n",
    "for pnti in range(-11,12):\n",
    "    plt.plot(pnti, y.subs(x,pnti), 'o')\n",
    "\n",
    "plt.show()"
   ]
  }
