   "source": [
    "v = np.arange(10,22,3)\n",
    "w = np.arange(5,15,3)\n",
    "\n",
    "dp1 = 0\n",
    "for i in range(0, len(v)):\n",
    "    dp1 = dp1 + v[i] * w[i]\n",
    "\n",
    "dp2 = np.sum(np.multiply(v,w))\n",
    "dp3 = np.dot(v,w)\n",
    "\n",
    "print(dp1,dp2,dp3)"
   ]
