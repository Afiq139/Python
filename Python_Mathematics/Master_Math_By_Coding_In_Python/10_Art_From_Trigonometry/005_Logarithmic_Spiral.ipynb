   "source": [
    "t = np.linspace(0,10*np.pi,1234)\n",
    "k = -3\n",
    "\n",
    "x = np.cos(t) * np.exp(t/k)\n",
    "y = np.sin(t) * np.exp(t/k)\n",
    "\n",
    "plt.plot(x,y,'r')\n",
    "plt.axis('square')\n",
    "plt.axis('off')\n",
    "\n",
    "plt.show()"
   ]
  }
